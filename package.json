{
	"name": "scribium",
	"version": "0.0.0",
	"private": true,
	"workspaces": [
		"apps/*",
		"packages/*"
	],
	"scripts": {
		"build": "turbo run build",
		"dev": "turbo run dev --parallel",
		"lint": "turbo run lint",
		"format": "prettier --write \"**/*.{ts,tsx,md}\""
	},
	"devDependencies": {
		"@scribium/eslint-config-scribium": "*",
		"eslint": "^8.27.0",
		"prettier": "^2.7.1",
		"turbo": "^1.6.3"
	},
	"engines": {
		"node": ">=14.0.0"
	},
	"packageManager": "yarn@1.22.19"
}
